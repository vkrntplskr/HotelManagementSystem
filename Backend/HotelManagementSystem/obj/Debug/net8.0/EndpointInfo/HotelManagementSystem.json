            "minLength": 0,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "phone": {
            "minLength": 1,
            "pattern": "^\\d{10}$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "Room": {
        "required": [
          "price",
          "roomNumber",
          "status",
          "type"
        ],
        "type": "object",
        "properties": {
          "roomId": {
            "type": "integer",
            "format": "int32"
          },
          "roomNumber": {
            "maximum": 100,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "type": {
            "minLength": 1,
            "type": "string"
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "status": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      }
